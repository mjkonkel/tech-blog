<!-- Loop over each object in the posts array -->
{{#each posts as |post| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <a href="/edit/{{post.id}}">{{post.title}}</a>
    </h2>
    {{!-- <p>Created by {{post.user.username}} on {{format_date post.created_at}}</p> --}}
   
  </div>

</div>
{{/each}}

<a href="/dashboard/add"><button>Create new post</button></a>